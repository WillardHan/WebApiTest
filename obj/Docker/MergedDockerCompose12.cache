services:
  identityserver:
    build:
      context: D:\GitWorkSpaces\WebApiTest
      dockerfile: IdentityServer/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: identityserver
    ports:
    - published: 55009
      target: 80
  webapitest:
    build:
      context: D:\GitWorkSpaces\WebApiTest
      dockerfile: WebApiTest/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: webapitest
    ports:
    - published: 55001
      target: 80
  webapitest2:
    build:
      context: D:\GitWorkSpaces\WebApiTest
      dockerfile: WebApiTest2/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: webapitest2
    ports:
    - published: 55002
      target: 80
version: '3.4'